  "bugs": {
    "url": "https://github.com/KushChauhanSS/express-server/issues"
  },
  "homepage": "https://github.com/KushChauhanSS/express-server#readme",
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/node": "^7.14.9",
    "@babel/preset-env": "^7.15.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^7.0.2",
    "nodemon": "^2.0.12",
    "tsc-watch": "^4.5.0",
    "tslint": "^6.1.3",
    "typescript": "^4.4.2"
  },
  "husky": {
    "hooks": {
